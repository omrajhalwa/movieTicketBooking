<%if(success&&success.length){%>
    <div class="alert alert-success mt-2 alert-dismissible fade show col-6 offset-3" role="alert" style="z-index: 10000 !important;">
       <%=success%>
       <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close">
     
      </button>
      </div>
      <%}%>
    
      <%if(error&&error.length){%>
       <div class="alert alert-danger mt-2 alert-dismissible fade show col-6 offset-3" role="alert" style="z-index: 10000 !important;">
          <%=error%>
          <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close">
        
         </button>
         </div>
         <%}%>